<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<div id="buttonList" class="row">
    <h1>Text Editor: </h1>
    <button id="quoteButton" type="button">&quot;</button>
    <button id="boldButton" type="button" value="B"><b>B</b></button>
    <button id="italicButton" type="button" ><i>I</i></button>
    <button id="underlineButton" type="button"><u>ABC</u></button>
    <button id="unorderedListButton" type="button" >ul</button>
    <button id="orderedListButton" type="button" >OL</button>
    <button id="imageButton" type="button" >IMG</button>
    <button id="linkButton" type="button">Link</button>
    <button id="displayContentsButton" type = "button">Display Contents</button>
</div>
